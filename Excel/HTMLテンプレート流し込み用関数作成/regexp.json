// jsonのような記述で、シングルクォートで括ることも、json内にコメントを書くことも可能です。
{
   "ダブルクォートを関数に置換"  :{"g": ['"', '"&char(34)&"']}
   ,"日付フォーマットで参照"    :{"g": ['#date#','"&TEXT(A1, "yyyy年m月d日")&"']}
   ,"テキストフォーマットで参照" :{"g": ['#text#','"&TEXT(A1, "@")&"']}
   ,"通常参照" :{"g": ['#s#','"&A1&"']}
   ,"文章全体を関数化"         :{"m": ['((.*\n*.*)*)','="$1"']}
}


